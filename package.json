{
  "name": "ant-extends",
  "version": "0.1.0",
  "description": "antd extends UI",
  "private": false,
  "main": "dist/npm/es5/index.js",
  "jsnext:main": "next.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "start": "cd site && webpack-dev-server --watch --progress",
    "typescript-test": "tsc --lib es2015 --moduleResolution node --project typings/typing-tests --skipLibCheck",
    "test": "flow check && BABEL_ENV=development jest",
    "jest": "jest",
    "lint": "eslint src libs site --quiet",
    "build:site": "cd build/site && webpack --progress",
    "prepubl¡sh": "./build/scripts/prepare-ts.sh && npm run genStyle",
    "genStyle": "babel src/**/*.less --out-dir dist/npm/es5/src/* --copy-files",
    "prepubl¡sh:babel": "./build/scripts/prepare.sh",
    "postpublish": "./build/scripts/release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leesren/ant-extends.git"
  },
  "keywords": [
    "antd",
    "UI",
    "React"
  ],
  "author": "ELEME Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leesren/ant-extends/issues"
  },
  "homepage": "https://github.com/leesren/ant-extends",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@babel/standalone": "^7.8.4",
    "@types/jest": "^25.1.3",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "clean-webpack-plugin": "^3.0.0",
    "codemirror": "^5.43.0",
    "core-js": "^2.6.3",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "marked": "^0.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mock-raf": "^1.0.1",
    "node-sass": "^4.11.0",
    "object-assign": "^4.1.1",
    "prismjs": "^1.15.0",
    "raw-loader": "^1.0.0",
    "react-hot-loader": "^4.12.19",
    "react-scroll-up": "^1.3.3",
    "regenerator-runtime": "^0.13.1",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.3",
    "style-loader": "^0.23.1",
    "typescript": "^3.3.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.2",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "antd": "^3.26.11",
    "classnames": "^2.2.6",
    "raf": "^3.4.1",
    "react": "^16.12.0",
    "react-click-outside": "^3.0.1",
    "react-dom": "^16.12.0",
    "react-transition-group": "^2.5.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "antd": "*"
  },
  "jest": {
    "testRegex": "(/jest/.+\\.(js|jsx)$)|_test.jsx?$",
    "testPathIgnorePatterns": [
      "<rootDir>/tests",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "\\.(png|eot|svg|ttf|woff|woff2)(\\?.+)?$": "<rootDir>/tests/jest/mock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/tests/jest/config.js"
  }
}
